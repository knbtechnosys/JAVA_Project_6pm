pipeline {
    agent any
    
    tools {
        maven 'Maven' 
    }
    
    stages {
        stage('Clone Repository') {
            steps {
                git branch: 'main', url: 'https://github.com/snehalatabarenkal/JAVA_Project_6pm'
            }
        }
        
        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
}

